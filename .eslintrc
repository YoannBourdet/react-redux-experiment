{
  env: {
    browser: true,
    commonjs: true,
    es6: true,
    node: true,
  },
  parserOptions: {
    ecmaVersion: 7,
    ecmaFeatures: {
      arrowFunctions: true,
      blockBindings: true,
      classes: true,
      defaultParams: true,
      destructuring: true,
      experimentalObjectRestSpread: true,
      forOf: true,
      generators: true,
      jsx: true,
      modules: false,
      objectLiteralComputedProperties: true,
      objectLiteralDuplicateProperties: true,
      objectLiteralShorthandMethods: true,
      objectLiteralShorthandProperties: true,
      octalLiterals: false,
      regexUFlag: false,
      regexYFlag: false,
      restParams: true,
      spread: true,
      superInFunctions: true,
      templateStrings: true,
      unicodeCodePointEscapes: true,
      globalReturn: false,
    },
    sourceType: 'module',
  },
  plugins: ['react'],
  parser: 'babel-eslint',
  globals: {
    wysihtml5: true,
    wysihtml5ParserRules: true,
  },
  rules: {
    'no-sparse-arrays': 0,
    'complexity': 0,
    'default-case': 0,
    'dot-location': 0,
    'guard-for-in': 0,
    'no-case-declarations': 0,
    'no-implicit-coercion': 0,
    'no-process-env': 0,
    'no-sequences': 0,
    'no-void': 0,
    'no-warning-comments': 0,
    'init-declarations': 0,
    'no-catch-shadow': 0,
    'no-restricted-globals': 0,
    'no-shadow': 0,
    'handle-callback-err': 0,
    'no-mixed-requires': 0,
    'no-process-exit': 0,
    'no-restricted-modules': 0,
    'no-confusing-arrow': 0,
    'prefer-reflect': 0,
    'id-blacklist': 0,
    'id-length': 0,
    'id-match': 0,
    'key-spacing': 0,
    'lines-around-comment': 0,
    'max-depth': 0,
    'max-nested-callbacks': 0,
    'max-statements': 0,
    'newline-after-var': 0,
    'newline-before-return': 0,
    'no-bitwise': 0,
    'no-continue': 0,
    'no-inline-comments': 0,
    'no-lonely-if': 0,
    'no-mixed-spaces-and-tabs': 0,
    'no-negated-condition': 0,
    'no-array-constructor': 0,
    'no-new-object': 0,
    'no-plusplus': 0,
    'no-restricted-syntax': 0,
    'no-ternary': 0,
    'one-var': 0,
    'padded-blocks': 0,
    'require-jsdoc': 0,
    'wrap-regex': 0,
    'react/display-name': 0,
    'react/forbid-prop-types': 0,
    'react/no-danger': 0,
    'react/no-set-state': 0,
    'react/no-string-refs': 0,
    'react/prefer-stateless-function': 0,
    'react/sort-prop-types': 1,
    'react/prop-types': 0,
    'react/jsx-handler-names': 0,
    'react/no-bind': 0,
    'react/jsx-no-literals': 0,
    'newline-per-chained-call': 0,
    'no-magic-numbers': 0,
    'sort-imports': 0,
    'no-extra-parens': 0,
    'no-cond-assign': 1,
    'no-console': 1,
    'no-constant-condition': 1,
    'no-control-regex': 1,
    'no-empty-character-class': 1,
    'no-extra-boolean-cast': 1,
    'no-extra-semi': 1,
    'no-regex-spaces': 1,
    'valid-jsdoc': [1, {
      requireParamDescription: false,
      requireReturnDescription: false,
    }],
    'accessor-pairs': 1,
    'array-callback-return': 1,
    'consistent-return': 1,
    'dot-notation': 1,
    'no-alert': 1,
    'no-div-regex': 1,
    'no-else-return': 1,
    'no-extra-bind': 1,
    'no-fallthrough': 1,
    'no-labels': 1,
    'no-new-wrappers': 1,
    'no-octal': 1,
    'no-unmodified-loop-condition': 1,
    'vars-on-top': 1,
    'yoda': 1,
    'global-require': 1,
    'no-new-require': 1,
    'no-path-concat': 1,
    'no-useless-constructor': 1,
    'prefer-arrow-callback': 1,
    'require-yield': 1,
    'func-names': 1,
    'max-len': [1, 100],
    'max-params': [1, 5],
    'no-underscore-dangle': 1,
    'operator-assignment': [1, 'always'],
    'sort-vars': 1,
    'react/no-did-mount-set-state': [1, 'allow-in-func'],
    'react/no-did-update-set-state': [1, 'allow-in-func'],
    'react/no-multi-comp': [1, { ignoreStateless: false }],
    'react/prop-types': [1, { ignore: ['children'] }],
    'react/sort-comp': 1,
    'react/jsx-sort-props': 1,
    'arrow-parens': 1,
    'comma-dangle': [2, 'always-multiline'],
    'no-debugger': 2,
    'no-dupe-args': 2,
    'no-dupe-keys': 2,
    'no-duplicate-case': 2,
    'no-empty': 2,
    'no-ex-assign': 2,
    'no-func-assign': 2,
    'no-inner-declarations': [2, 'both'],
    'no-invalid-regexp': 2,
    'no-irregular-whitespace': 2,
    'no-negated-in-lhs': 2,
    'no-obj-calls': 2,
    'no-unexpected-multiline': 2,
    'no-unreachable': 2,
    'use-isnan': 2,
    'valid-typeof': 2,
    'block-scoped-var': 2,
    'curly': [2, 'all'],
    'eqeqeq': 2,
    'no-caller': 2,
    'no-empty-function': [2, { allow: ['arrowFunctions'] }],
    'no-empty-pattern': 2,
    'no-eq-null': 2,
    'no-eval': 2, // [2, { allowIndirect: true }],
    'no-extend-native': 2,
    'no-extra-label': 2,
    'strict': [2, 'global'],
    'no-implicit-globals': 2,
    'no-implied-eval': 2,
    'no-invalid-this': 2,
    'no-iterator': 2,
    'no-lone-blocks': 2,
    'no-loop-func': 2,
    'no-multi-spaces': 2,
    'no-multi-str': 2,
    'no-native-reassign': 2,
    'no-new': 2,
    'no-new-func': 2,
    'no-octal-escape': 2,
    'no-param-reassign': [2, { props: false }],
    'no-floating-decimal': 2,
    'no-proto': 2,
    'no-redeclare': 2,
    'no-return-assign': 2,
    'no-script-url': 2,
    'no-self-assign': 2,
    'no-self-compare': 2,
    'no-throw-literal': 2,
    'no-unused-expressions': 2,
    'no-unused-labels': 2,
    'no-useless-call': 2,
    'no-useless-concat': 2,
    'no-with': 2,
    'radix': [2, 'as-needed'],
    'wrap-iife': [2, 'inside'],
    'no-delete-var': 2,
    'no-label-var': 2,
    'no-shadow-restricted-names': 2,
    'no-undef': 2,
    'no-undef-init': 2,
    'no-undefined': 2,
    'no-unused-vars': [2, {
      vars: 'local', args: 'after-used',
      argsIgnorePattern: '^unused', varsIgnorePattern: '^unused',
    }],
    'no-use-before-define': 2,
    'callback-return': 2,
    'no-restricted-imports': [2, 'fs'],
    'no-sync': 2,
    'arrow-body-style': [2, 'as-needed'],
    'arrow-spacing': 2,
    'constructor-super': 2,
    'generator-star-spacing': 2,
    'no-class-assign': 2,
    'no-const-assign': 2,
    'no-dupe-class-members': 2,
    'no-new-symbol': 2,
    'no-this-before-super': 2,
    'no-var': 2,
    'object-shorthand': [2, 'always'],
    'prefer-const': 2,
    'prefer-rest-params': 2,
    'prefer-spread': 2,
    'prefer-template': 2,
    'template-curly-spacing': [2, 'never'],
    'yield-star-spacing': [2, { before: true, after: false }],
    'array-bracket-spacing': [2, 'never'],
    'block-spacing': [2, 'always'],
    'brace-style': [2, '1tbs', { allowSingleLine: false }],
    'camelcase': [2, { properties: 'always' }],
    'comma-spacing': [2, { before: false, after: true }],
    'comma-style': [2, 'last'],
    'computed-property-spacing': [2, 'never'],
    'consistent-this': [2, 'that'],
    'eol-last': 2,
    'func-style': [2, 'expression', { allowArrowFunctions: true }],
    'indent': [2, 2, { SwitchCase: 1 }],
    'jsx-quotes': [2, 'prefer-double'],
    'keyword-spacing': 2,
    'linebreak-style': [2, 'unix'],
    'new-cap': 2,
    'new-parens': 2,
    'no-multiple-empty-lines': [2, { max: 1 }],
    'no-nested-ternary': 2,
    'no-spaced-func': 2,
    'no-trailing-spaces': 2,
    'no-unneeded-ternary': 2,
    'no-whitespace-before-property': 2,
    'object-curly-spacing': [2, 'always'],
    'one-var-declaration-per-line': [2, 'always'],
    'operator-linebreak': [2, 'after'],
    'quote-props': [2, 'as-needed'],
    'quotes': [2, 'single', 'avoid-escape'],
    'semi': [2, 'always'],
    'semi-spacing': [2, { before: false, after: true }],
    'space-before-blocks': [2, 'always'],
    'space-before-function-paren': [2, 'never'],
    'space-in-parens': [2, 'never'],
    'space-infix-ops': 2,
    'space-unary-ops': [2, { words: true, nonwords: false }],
    'spaced-comment': [2, 'always'],
    'react/no-is-mounted': 2,
    'react/no-unknown-property': 2,
    'react/prefer-es6-class': [2, 'always'],
    'react/react-in-jsx-scope': 2,
    'react/require-extension': 2,
    'react/self-closing-comp': 2,
    'react/wrap-multilines': 2,
    'react/no-deprecated': 2,
    'react/no-direct-mutation-state': 2,
    'react/jsx-boolean-value': [2, 'never'],
    'react/jsx-closing-bracket-location': [2, 'line-aligned'],
    'react/jsx-curly-spacing': [2, 'never'],
    'react/jsx-equals-spacing': [2, 'never'],
    'react/jsx-indent-props': [2, 2],
    'react/jsx-indent': [2, 2],
    'react/jsx-key': 2,
    'react/jsx-max-props-per-line': [2, { maximum: 3 }],
    'react/jsx-no-duplicate-props': [2, { ignoreCase: false }],
    'react/jsx-no-undef': 2,
    'react/jsx-pascal-case': 2,
    'react/jsx-space-before-closing': [2, 'always'],
    'react/jsx-uses-react': 2,
    'react/jsx-uses-vars': 2,
  }
}
